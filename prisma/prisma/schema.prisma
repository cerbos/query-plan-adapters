// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Resource {
  id        String  @id @default(cuid())
  aString   String
  aNumber   Int
  aBool     Boolean
  creator   User    @relation(fields: [creatorId], references: [id], name: "creator")
  owners    User[]
  creatorId String
}

model User {
  id      String     @id @default(cuid())
  aString String
  aNumber Int
  aBool   Boolean
  creator Resource[] @relation(name: "creator")
  owners  Resource[]
}
