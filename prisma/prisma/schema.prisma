// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Resource {
  id      Int     @id @default(autoincrement())
  aString String
  aNumber Int
  aBool   Boolean
  owners  User[]
}

model User {
  id         Int       @id @default(autoincrement())
  resources  Resource? @relation(fields: [resourceId], references: [id])
  resourceId Int?
  aString    String
  aNumber    Int
  aBool      Boolean
}
