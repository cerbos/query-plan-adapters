// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Resource {
  id      String  @id @default(cuid())
  aString String
  aNumber Int
  aBool   Boolean
  owners  User[]
}

model User {
  id        String     @id @default(cuid())
  aString   String
  aNumber   Int
  aBool     Boolean
  resources Resource[]
}
